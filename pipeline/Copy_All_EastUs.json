				"type": "SetVariable",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"variableName": "container_list",
					"value": [
						"ameshousing",
						"chexpert",
						"food101",
						"lendingclub",
						"trash-objects-in-context"
					]
				}
			},
			{
				"name": "MoveContainer",
