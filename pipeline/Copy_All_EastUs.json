										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "Dest_ADLS_All_Eastus",
									"type": "DatasetReference",
									"parameters": {
										"container_name": {
											"value": "@item()",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"variables": {
			"container_list": {
				"type": "Array"
			}
		},
		"annotations": []
	}
}